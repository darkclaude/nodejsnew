<!doctype html>
<html ng-app >
<head>
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="css/bootstrap.min.css">
 <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular.min.js"></script>
 <script src="control\Controller.js"></script>

<title>
    Admin/User List
</title>
</head>
<body>
<div class="container" ng-controller="AppCtrl">
 <h1>User List Data</h1>
 <table class="table">
 <thead>
 <tr>
    <th>User ID Number</th>
    <th>Username</th>
    <th>Action</th>
 </tr>
</thead>
<tbody>
<tr>
    <td><input class="form-control" ng-model="contact.name"></td>
    <td><input class="form-control" ng-model="lol"></td>
    <td><button class="btn btn-primary" ng-click="addContact()">Admin List</button></td>
    <td><button class="btn btn-primary" ng-click="ul()">User List</button></td>
    <td><button class="btn btn-info" ng-click="update()">Update</button>&nbsp;&nbsp;<button class= "btn btn-info" ng-click="deselect()">Logout</button></td>

</tr>
    <tr ng-repeat="contact in contactlist">
    <td>{{contact._id}}</td>
    <td>{{contact.local.username}}</td>
    <td><button class="btn btn-danger" ng-click="remove(contact._id)">Remove</button></td>
    <td><button class="btn btn-warning" ng-click="edit(contact._id)">Edit</button></td>
    <td><button class="btn btn-success" ng-click="ma(contact._id)"  ng-show={{v}}>Make Admin</button></td>
    <td><button class="btn btn-success" ng-click="rv(contact._id)"  ng-show={{v}}>Revoke Token</button></td

    </tr>
</tbody>
  </table>
 </div>
</body>


</html>